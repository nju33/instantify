{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "not < 0.3%"],
      },
      "modules": false
    }],
    "flow"
  ],
  "plugins": [
    "external-helpers",
    "transform-runtime",
    "transform-object-rest-spread",
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": ["last 2 versions", "not < 0.3%"],
          }
        }],
        "flow"
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties"
      ],
    }
  }
}
